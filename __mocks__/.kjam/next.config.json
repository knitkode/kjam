{
  "i18n": {
    "locales": ["en", "it"],
    "defaultLocale": "en"
  },
  "redirects": [
    {
      "source": "/it/spaces",
      "destination": "/it/spazi",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/spaces/:slug*",
      "destination": "/it/spazi/:slug*",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/projects",
      "destination": "/it/progetti",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/projects/:slug*",
      "destination": "/it/progetti/:slug*",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/en/spaces/outdoor",
      "destination": "/spaces/living-outside",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/en/spaces/outdoor/:slug*",
      "destination": "/spaces/living-outside/:slug*",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/spaces/outdoor",
      "destination": "/it/spazi/all-aperto",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/spaces/outdoor/:slug*",
      "destination": "/it/spazi/all-aperto/:slug*",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/en/about",
      "destination": "/about-us",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/about",
      "destination": "/it/chi-siamo",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/events",
      "destination": "/it/eventi",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/events/:slug*",
      "destination": "/it/eventi/:slug*",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/spaces/indoor",
      "destination": "/it/spazi/indoor",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/en/spaces/outdoor/by-season",
      "destination": "/spaces/living-outside/by-season",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/en/spaces/outdoor/by-season/:slug*",
      "destination": "/spaces/living-outside/by-season/:slug*",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/spaces/outdoor/by-season",
      "destination": "/it/spazi/all-aperto/stagioni",
      "permanent": false,
      "locale": false
    },
    {
      "source": "/it/spaces/outdoor/by-season/:slug*",
      "destination": "/it/spazi/all-aperto/stagioni/:slug*",
      "permanent": false,
      "locale": false
    }
  ],
  "rewrites": [
    {
      "source": "/spazi",
      "destination": "/spaces"
    },
    {
      "source": "/spazi/:path*",
      "destination": "/spaces/:path*"
    },
    {
      "source": "/progetti",
      "destination": "/projects"
    },
    {
      "source": "/progetti/:path*",
      "destination": "/projects/:path*"
    },
    {
      "source": "/spaces/living-outside",
      "destination": "/spaces/outdoor"
    },
    {
      "source": "/spaces/living-outside/:path*",
      "destination": "/spaces/outdoor/:path*"
    },
    {
      "source": "/spazi/all-aperto",
      "destination": "/spaces/outdoor"
    },
    {
      "source": "/spazi/all-aperto/:path*",
      "destination": "/spaces/outdoor/:path*"
    },
    {
      "source": "/about-us",
      "destination": "/about"
    },
    {
      "source": "/chi-siamo",
      "destination": "/about"
    },
    {
      "source": "/eventi",
      "destination": "/events"
    },
    {
      "source": "/eventi/:path*",
      "destination": "/events/:path*"
    },
    {
      "source": "/spazi/indoor",
      "destination": "/spaces/indoor"
    },
    {
      "source": "/spaces/living-outside/by-season",
      "destination": "/spaces/outdoor/by-season"
    },
    {
      "source": "/spaces/living-outside/by-season/:path*",
      "destination": "/spaces/outdoor/by-season/:path*"
    },
    {
      "source": "/spazi/all-aperto/stagioni",
      "destination": "/spaces/outdoor/by-season"
    },
    {
      "source": "/spazi/all-aperto/stagioni/:path*",
      "destination": "/spaces/outdoor/by-season/:path*"
    }
  ]
}
