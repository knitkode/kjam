{
  "root": "packages/serializer-next",
  "sourceRoot": "packages/serializer-next/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/serializer-next",
        "main": "packages/serializer-next/src/index.ts",
        "tsConfig": "packages/serializer-next/tsconfig.lib.json",
        "assets": ["packages/serializer-next/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/serializer-next/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/serializer-next"],
      "options": {
        "jestConfig": "packages/serializer-next/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
