{
  "root": "packages/action-next",
  "sourceRoot": "packages/action-next",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "outputs": ["{workspaceRoot}/actions/next"]
    },
    "_build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/actions/next"],
      "options": {
        "commands": [
          "ncc build packages/action-next/actions/action-next.ts -o actions/next --minify --no-cache --no-source-map-register --license licenses.txt"
        ],
        "parallel": true
      }
    },
    "lint": {},
    "test": {}
  }
}
