{
  "name": "kjam",
  "version": "1.6.11",
  "license": "MIT",
  "scripts": {
    "start": "nx affected:test --all --parallel --maxParallel 10 --watch",
    "build": "nx run-many --target=build --all=true",
    "lint": "nx run-many --target=lint --all=true --fix",
    "test": "nx test",
    "format": "nx format:write",
    "clean": "rm -rf ./tmp && rm -rf ./dist && rm -rf ./node_modules/.cache/nx",
    "prepare": "husky install",
    "test-action-next": "nx run action-next:build && KJAM_GIT=amicidellaschola/content-www/main KJAM_FOLDER=../../LaSchola/content-www KJAM_SKIP_GIT=true node ./dist/packages/action-next/index.js",
    "test-cli": "nx run cli:build && KJAM_GIT=amicidellaschola/content-www/main KJAM_FOLDER=../../LaSchola/content-www node ./dist/packages/cli watch",
    "link": "cd ./dist/packages/cli && npm link && cd ../core && npm link && cd ../next && npm link  && cd ../next-ui && npm link"
  },
  "private": true,
  "dependencies": {
    "core-js": "^3.36.0",
    "regenerator-runtime": "^0.14.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-angular": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@nx/eslint": "^18.0.8",
    "@nx/eslint-plugin": "^18.0.8",
    "@nx/jest": "^18.0.8",
    "@nx/js": "^18.0.8",
    "@nx/node": "^18.0.8",
    "@nx/react": "^18.0.8",
    "@nx/rollup": "^18.0.8",
    "@nx/workspace": "^18.0.8",
    "@swc/core": "^1.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.2.2",
    "nx": "^18.0.8",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,ts,tsx}": [
      "prettier --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
