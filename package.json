{
  "name": "kjam",
  "version": "1.5.2",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "start": "nx affected:test --all --parallel --maxParallel 10 --watch",
    "prepare": "husky install",
    "build": "nx run-many --target=build --all=true",
    "format": "clear && npx prettier --write **/*.{js,json,css,ts,tsx}",
    "lint": "clear && npx eslint --fix packages/**/*.{ts,tsx}",
    "clean": "rm -rf ./node_modules/.cache/nx",
    "test-action-next": "nx run action-next:build && KJAM_GIT=amicidellaschola/content-www/main KJAM_FOLDER=../../LaSchola/content-www KJAM_SKIP_GIT=true node ./dist/packages/action-next/index.js",
    "test-cli": "nx run cli:build && KJAM_GIT=amicidellaschola/content-www/main KJAM_FOLDER=../../LaSchola/content-www node ./dist/packages/cli watch"
  },
  "private": true,
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@babel/code-frame": "^7.16.7",
    "@types/dotenv": "^8.2.0",
    "chokidar": "^3.5.3",
    "commander": "^9.0.0",
    "core-js": "^3.21.1",
    "fdir": "^5.2.0",
    "fs-extra": "^10.0.0",
    "get-port": "^6.1.1",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "next": "^12.1.0",
    "next-mdx-remote": "^4.0.0",
    "next-translate": "^1.3.4",
    "probe-image-size": "^7.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "serve-handler": "^6.1.3",
    "simple-git": "^3.2.6",
    "tslib": "^2.3.1",
    "unist-util-remove": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-angular": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@e-square/nx-ncc": "^1.1.0",
    "@nrwl/cli": "^13.8.3",
    "@nrwl/eslint-plugin-nx": "^13.8.3",
    "@nrwl/jest": "^13.8.3",
    "@nrwl/js": "^13.8.3",
    "@nrwl/linter": "^13.8.3",
    "@nrwl/node": "^13.8.3",
    "@nrwl/react": "^13.8.3",
    "@nrwl/web": "^13.8.3",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.143",
    "@swc/helpers": "^0.3.3",
    "@types/babel__code-frame": "^7.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.19",
    "@types/probe-image-size": "^7.0.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/serve-handler": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.10.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
